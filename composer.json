{
    "name": "daydevelops/vote",
    "description": "A package built for laravel applications which allows items to be voted on. Items can be upvoted or downvoted which counts towards a total score.",
    "license": "MIT",
    "authors": [
        {
            "name": "Daydevelops",
            "email": "adamday@daydevelops.com",
            "homepage": "https://daydevelops.com"
        }
    ],
    "homepage": "https://github.com/daydevelops/vote",
    "keywords": ["Laravel", "Vote"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "fzaninotto/faker": "~1.4",
        "laravel/laravel": "~7.0 | ~6.0",
        "doctrine/dbal": "^2.5",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Daydevelops\\Vote\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Daydevelops\\Vote\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Daydevelops\\Vote\\VoteServiceProvider"
            ]
        }
    }
}
